<div class="register-photo">
    <div class="form-container">
            <form method="post" id="myForm">
            <h2 class="text-center"><strong>Create</strong> Contest</h2>
            <div class="form-group"><input type="text" name="Product" placeholder="Name Of Product" class="form-control" /></div>

             <div class="form-group input-group mb-3 "> 
                <select class="custom-select form-control" id="inputGroupSelect01">
                <option selected>Categories</option>
                <option value="1">Electronics</option>
                <option value="2">Fitness</option>
                    <option value="2">Beauty Products</option>
                </select>
            </div>
                 
        
            <div class="row">
                <div class="col-md-6 form-group"><input type="date" name="Start" placeholder="Start Date:" class="form-control" /></div>
                <div class="col-md-6 form-group"><input type="date" name="End" placeholder="End Date:" class="form-control" /></div>
            </div>
            <div class="form-group"><input type="text" naDiscriptionme="Discription" placeholder="Discription About Product " class="form-control" /></div>


            <div class="form-group"><input type="text" name="Geographies" placeholder="Contest Available for Locations" class="form-control" /></div>
            <div class="form-group"><label class="form-control">Terms and Conditions(PDF Only): <input type="file" name="Start" accept="application/pdf" /></label></div>
        
            <div class="row">
                <div class="col-md-6 form-group" >
                    <input class="btn btn-primary btn-block" type="submit" onclick="myFunction()" value="Reset Form"></input>
                </div>
                
                <div class="col-md-6 form-group">
                   <%= link_to "Prize Details", prize_path, class:'btn btn-primary btn-block'%>
                </div>
            </div>

        </form>
    </div>
</div>

<script>
    function myFunction() {
    document.getElementById("myForm").reset();
}
</script>